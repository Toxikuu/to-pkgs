#!/to

n="sysklogd"
v="2.7.2"
r="1"
a="Programs for logging system messages"
m="Tox"
l="GPL-2.0-or-later"
t="log core utils"
u="https://github.com/troglobit/$n.git"

s=(
    "${u%.git}/releases/download/v$v/$n-$v.tar.gz"
)

d=(
    "glibc"
)

b() {

_cfg=(
    --runstatedir=/run
    --without-logger
)

def
install -vDm644 "$A/syslog.conf" -t "$D/etc/"

}
