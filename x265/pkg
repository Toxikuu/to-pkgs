#!/to

n="x265"
v="5130ab3ef90658526c26b51c265992d2088b5360"
a="H.265/HEVC encoder"
m="Tox"
l="GPL"
t="codec"
u="https://bitbucket.org/multicoreware/${n}_git.git"

s=(
    "$u"
)

d=(
    "gcc"
    "b,git"
    "b,cmake"
    "b,ninja"
    "b,nasm"
)

b() {

gco
with cmake
cd source

_cfg=(
    -D ENABLE_SHARED=TRUE
)

def
rm -vf "$D/usr/lib/lib$n.a"

}
