#!/to

n="jaq"
v="2.3.0"
r="2"
tag="v$v"
a="Command-line JSON processor"
m="Tox"
l="MIT"
t="utils lang cli"
u="https://github.com/01mf02/$n.git"

s="$u"
d=(
    "gcc"
    "glibc"
    "b,rust"
)

b() {

RUSTFLAGS="$(echo "$RUSTFLAGS" | sed -e 's|-C lto=\w\+||g' -e 's|-C embed-bitcode=true||g')"
CFLAGS+=" -ffat-lto-objects"

cbr

install -vDm755 "target/release/$n" -t "$D/usr/bin/"
ln -sv "jaq" "$D/usr/bin/jq"

}

t() {

cargo test

}
