#!/to

n="st"
v="0.9.2"
a="Suckless terminal"
m="Tox"
l="MIT"
t="suckless term"
u="https://git.suckless.org/$n"

s="${u/git/dl}/$n-$v.tar.gz"
d="libxft"

b() {

with c99
il

mkdir -pv       "$D/usr/src/$n"
cp -avf {,.}*   "$D/usr/src/$n"

mk X11INCDIR=/usr/include/X11 X11LIB=/usr/lib/X11
mi PREFIX=/usr

}
