#!/to

n="neovim"
v="5661f74ab2a6ef0c497ef2ea49bc58ea89b6ab6b"
a="Continuation and extension of Vim"
m="Tox"
l=(
    "Apache-2.0"
    "Custom"
)
t="editor utils tui"
u="https://github.com/$n/$n.git"

s="$u"

d=(
    "b,git"
    "b,ninja"
    "b,cmake"
    "libuv"
    "luajit"
)

b() {

with cn

gco

# Build bundled dependencies
cmake -S cmake.deps -B .deps -G Ninja -D CMAKE_BUILD_TYPE=Release
cmake --build .deps

def

}

t() {

# Sanity checks
bin/nvim --version
bin/nvim --headless -u NONE -i NONE +q

}
