#!/to

n="neovim"
v="d25eb246efc5d8886c72c2e3f0107decf79f625d"
a="Continuation and extension of Vim"
m="Tox"
l=(
    "Apache-2.0"
    "Custom"
)
t="editor utils tui"
u="https://github.com/$n/$n.git"

s=(
    "$u"
)

d=(
    "b,git"
    "b,ninja"
    "b,cmake"
    "libuv"
    "luajit"
)

b() {

with cn

gco

# Build bundled dependencies
cmake -S cmake.deps -B .deps -G Ninja -D CMAKE_BUILD_TYPE=Release
cmake --build .deps

def

}

t() {

# Sanity checks
bin/nvim --version
bin/nvim --headless -u NONE -i NONE +q

}
