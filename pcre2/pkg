pcre2@10.45-1

a="Perl-Compatible Regular Expressions"
m="Tox"
l=(
    "BSD-2-Clause"
    "BSD-3-Clause WITH ${n^^}-exception"
)
t="core lib"
u="https://github.com/${n^^}Project/$n.git"

s=(
    "${u%.git}/releases/download/$n-$v/$n-$v.tar.bz2"
)

d=(
    "bzip2"
    "glibc"
    "zlib"
)

b() {

_cfg=(
    --enable-unicode
    --enable-jit
    --enable-$n-16
    --enable-$n-32
    --enable-${n}grep-libz
    --enable-${n}grep-libbz2
)

def

}
