nushell@0.106.1-3

a="A new type of shell"
m="Tox"
l="MIT"
t="sh"
u="https://github.com/$n/$n.git"
vf="gr | sed 's,_,.,g' | sort -V"

s="$u"
d=(
    "gcc"
    "glibc"
    "openssl"
    "zlib"
    "b,rust"
)

b() {

rm -vf rust-toolchain.toml
export CFLAGS+=" -ffat-lto-objects"

cbr

find target/release \
    -maxdepth 1     \
    -mindepth 1     \
    -type f         \
    -name "nu*"     \
    -exec install -vDm755 -t "$D/usr/bin/" {} +

install -vDm644 README.md -t "$D/usr/share/doc/$n/"

}
