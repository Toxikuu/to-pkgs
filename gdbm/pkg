#!/to

n="gdbm"
v="0d3d46cdee38c7b00a107c52875ffb36c2bc7170"
a="The GNU Database Manager"
m="Tox"
l="GPL-3.0-or-later"
t="core lib db"
u="https://https.git.savannah.gnu.org/git/$n.git"

s="$u"
d="glibc"

b() {

# Compatibility library wanted by some packages
_cfg=(
    --enable-libgdbm-compat
)

autoreconf -fiv
def

}

t() {

mk check

}
