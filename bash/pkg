#!/to

n="bash"
v="5.3-rc2"
a="The Bourne-Again Shell"
m="Tox"
l="GPL-3.0-or-later"
t="shell core lang"
u="https://ftp.gnu.org/gnu/$n/?C=M;O=D"
vf="curl -s '$u' | grep '$n-' | grep -Ev -- '-beta|-alpha|diff|doc|\.sig|patches' | head -n1 | cut -d '\"' -f8 | sed 's,.tar.gz,,'"

s=(
    "https://ftp.gnu.org/gnu/$n/$n-$v.tar.gz"
)

d=(
    "glibc"
    "ncurses"
    "readline"
)

b() {

_cfg=(
    --without-bash-malloc
    --with-installed-readline
)

def

}
