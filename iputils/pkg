#!/to

n="iputils"
v="23b06385444fba29c898370ae6f297e41c11667b"
a="The 'ping' networking utility"
m="Tox"
l=("BSD-3-Clause" "GPL-2.0-or-later")
t="net utils cli"
u="https://github.com/$n/$n.git"

s="$u"

d=(
    "b,meson"
    "b,samurai"
    "b,iproute2"
    "libcap"
    "libidn2"
)

b() {

with meson

_cfg=(
    -D USE_CAP=true
    -D USE_IDN=true

    -D BUILD_PING=true

    -D BUILD_ARPING=false
    -D BUILD_CLOCKDIFF=false
    -D BUILD_TRACEPATH=false
    -D BUILD_MANS=false
    -D BUILD_HTML_MANS=false

    -D INSTALL_SYSTEMD_UNITS=false
    -D USE_GETTEXT=false
    -D SKIP_TESTS=true
)

def

}
