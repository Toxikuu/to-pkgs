#!/to

n="xml-parser"
v="2.47"
a="Perl interface to Expat"
m="Tox"
l=(
    "Artistic-2.0"
)
t="lib"
u="https://github.com/cpan-authors/XML-Parser.git"
vf="gr | sed -e 's,^v,,' -e 's,_,.,g' | sort -V"

s=(
    "https://cpan.metacpan.org/authors/id/T/TO/TODDR/XML-Parser-$v.tar.gz"
)

d=(
    "expat"
    "perl"
)

b() {

perl Makefile.PL
mk
mi

install -vDm644 LICENSE -t "$D/usr/share/licenses/$n"

}

t() {

mk test

}
